
pipeline{
	agent any 
	tools {maven 'maven'}
		stages{
			stage('git-clone') {
				steps{
					checkout([$class: 'GitSCM', branches: [[name: '*/main']], extensions: [], userRemoteConfigs: [[credentialsId: 'git-Jenkins', url: 'https://github.com/ETECH-Co/module2_ci.git']]])
				}
			}
			stage('Hello-Etech'){
				steps{
				sh 'git version'
				}
			}
			stage('codebuild'){
				steps{
				sh 'mvn clean package -DskipTests=true'
				sh 'mvn -v'
				}
			}
		} 
}
